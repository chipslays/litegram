{
    "name": "chipslays/litegram",
    "description": "Simple, flexible, modular library for Telegram Bot Api.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "telegram",
        "bot",
        "api"
    ],
    "authors": [
        {
            "name": "Chipslays",
            "email": "hello@chipslays.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "chipslays/event": "1.0.*",
        "chipslays/sauce": "^1.0",
        "chipslays/phrase": "^1.0",
        "chipslays/collection": "^1.0",
        "illuminate/database": "^8.29",
        "wamania/php-stemmer": "2.0",
        "respect/validation": "^2.2",
        "opis/closure": "^3.6",
        "chipslays/pastly-api": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Litegram\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
